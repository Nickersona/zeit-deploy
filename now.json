{
  "version": 2,
  "name": "zeit-deploy",
  "build": {
    "env": {
      "CONTENTFUL_ACCESSTOKEN": "@contentful-access-token",
      "CONTENTFUL_ENVIRONMENT": "@contentful-environment",
      "CONTENTFUL_SPACE_ID": "@contentful-space-id"
    }
  },
  "builds": [
    { "src": "package.json", "use": "@now/next" },
    { "src": "lambdas/*.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/utils/(.*)", "dest": "/lambdas/$1" }
  ]
}